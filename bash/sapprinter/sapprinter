#!/bin/bash

former=$1
shift
printer=$1
shift 
in_file=$1
shift

if [ ${former} = "psnup" ];then
		cat ${in_file} | pdf2ps | psnup -1 -f | ${printer} $@
elif [ ${former} = "pdftk" ];then
	out_file="/var/tmp/out.pdf"
	pdftk ${in_file} cat 1-endN output ${out_file}
	${printer} $@ ${out_file}
	rm ${out_file}
fi
